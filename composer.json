{
  "name": "turbopanel/turbopanel",
  "description": "TurboPanel Web Hosting Environment",
  "version": "1.0.0.36",
  "license": "GPL-2.0",
  "type": "project",
  "homepage": "https://turbopanel.io/",
  "require": {
    "php": "^8.3",
    "codeigniter4/framework": "4.5.1"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9",
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "^10.5.16"
  },
  "autoload": {
    "psr-4": {},
    "exclude-from-classmap": [
      "**/Database/Migrations/**"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Support\\": "tests/_support"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit"
  }
}
